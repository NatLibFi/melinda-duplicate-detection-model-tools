{
  "name": "duplicate-detection-model-tools",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "postinstall": "cd melinda-deduplication-common && npm install",
    "test": "npm run flow",
    "test:unit": "BABEL_ENV=test mocha --compilers js:babel-core/register \"melinda-deduplication-common/{,!(node_modules)/**/}*.spec.js\"",
    "test:watch": "npm run test:unit -- -w",
    "flow": "flow",
    "train-model": "babel-node bin/train-model | tee training.txt",
    "test-model": "babel-node bin/validate-model",
    "test-model-trainingset": "babel-node bin/validate-model data-sets/trainingSet.json",
    "cross-validate-model": "babel-node bin/validate-model data-sets/crossValidationSet.json",
    "extract-false-positives": "babel-node bin/extract-false-positives.js",
    "display-false-positives": "babel-node bin/display-items.js"
  },
  "author": "Pasi Tuominen <pasi.tuominen@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.1",
    "concurrently": "^3.5.0",
    "eslint": "^4.1.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.6.0",
    "flow-bin": "^0.53.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "sinon": "^3.2.1"
  },
  "dependencies": {
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "synaptic": "1.0.12"
  }
}
